    "combos = []\n",
    "def make_combos(x, str):\n",
    "    if x == None:\n",
    "        return\n",
    "    combos.append(str[x])\n",
    "    make_combos(x+1, str)\n",
    "\n",
    "def print_combo(str):\n",
    "    n = len(str)\n",
    "    \n",
    "    for x in range(0,n):\n",
    "        make_combos(x, str)\n",
    "        print(combos)    \n",
    "    "
   ]
